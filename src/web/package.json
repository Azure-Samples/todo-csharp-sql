{
  "name": "react-fluent",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fluentui/react": "^8.41.4",
    "@microsoft/applicationinsights-react-js": "^3.2.1",
    "@microsoft/applicationinsights-web": "^2.7.1",
    "axios": "^0.24.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.0.2",
    "web-vitals": "^1.1.2"
  },
  "scripts": {
    "start": "if-node-version '>= 17' && react-scripts --openssl-legacy-provider start || react-scripts start",
    "build": "if-node-version '>= 17' && react-scripts --openssl-legacy-provider build || react-scripts build",
    "test": "if-node-version '>= 17' && react-scripts --openssl-legacy-provider test || react-scripts test",
    "eject": "if-node-version '>= 17' && react-scripts --openssl-legacy-provider eject || react-scripts eject",
    "lint": "eslint ./src --ext .ts,.tsx"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.15.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.11",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/react-router-dom": "^5.3.2",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "eslint-config-react-app": "^6.0.0",
    "if-node-version": "^1.1.1",
    "immer": "^9.0.7",
    "react-scripts": "^5.0.0",
    "typescript": "^4.5.2"
  }
}
